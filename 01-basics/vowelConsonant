#include<iostream>
using namespace std;
int main(){
    char ch;
    cout<<"Enter The Character : ";
    cin>>ch;

    int ascii = (int)ch;
    if((ascii>=97 && ascii<=122) || (ascii>=65 && ascii<=90)){
        if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u'){
            cout<<"The Character Is Vowel";
        }else{
            cout<<"Character is Consonant";
        }
    }
}
