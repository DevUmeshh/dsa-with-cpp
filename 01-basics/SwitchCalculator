#include<iostream>
using namespace std;
int main(){
    float a,b;
    char op;
    cout<<"Enter The Problem : ";
    cin>>a>>op>>b;

    switch(op){
        case '+':
            cout<<a+b;
            break;
        case '-':
            cout<<a-b;
            break;
        case '*':
            cout<<a*b;
            break;
        case '/':
            cout<<a/b;
            break;
    }




}
