#include<iostream>
using namespace std;
int main(){
    int x,y;
    cout<<"enter points : ";
    cin>> x >> y;
    if(x>0 && y==0){
        cout<<"the point lies on the x - axis";
}else if(y>0 && x==0){
    cout<<"the point lies on the y - axis";
}else if(x==0 || y ==0){
    cout<<"point is on origin";
}else {
    cout<<"another points";
}
}
