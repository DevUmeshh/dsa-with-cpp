#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cout<<"Enter A : ";
    cin>>a;
    cout<<"Enter B : ";
    cin>>b;
    cout<<"Enter C : ";
    cin>>c;

    if (a<b && a<c ){
        cout<<"A has least marks";
    }else if(b<a && b<c){
        cout<<"B has least marks";
    }else{
        cout<<"C has least marks";
    }
}
