#include<iostream>
using namespace std;
int main(){
    int a,b;
    cout<<"Enter value of base : ";
    cin>>a;
    cout<<"Enter value of exponent : ";
    cin>>b;
    bool flag = true;//true means positive power
    if(b<0){
        bool flag =false;//false means negative power
        b = -b;
        
    }
    int power = 1;
    for(int i=1;i<=b;i++){
        power*=a;
    }
    if(flag==false) power = 1.0/power;
        b = -b;
    if(a==0 && b==0) cout<<"not defined"<<endl;
    else cout<<power<<endl;
}
